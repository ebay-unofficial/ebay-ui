{
  "name": "ebay-ui",
  "description": "Simple UI where you can easily click together an API request",
  "version": "0.1.0",
  "dependencies": {
    "@ebay/ebayui-core": "^1.3.1",
    "@ebay/skin": "^6.3.2",
    "marko": "^4.14.16",
    "marko-starter": "^2.0.4",
    "marko-widgets": "^7.0.1"
  },
  "devDependencies": {
    "marko-prettyprint": "^2.0.0",
    "prettier": "^1.15.3"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ebay-unofficial/ebay-ui"
  },
  "scripts": {
    "build": "NODE_ENV=production marko-starter build",
    "format": "prettier src/**/*.{js,css,less} --write && marko-prettyprint src",
    "serve": "NODE_ENV=production marko-starter serve-static",
    "start": "marko-starter server"
  }
}
